class Base
{
	int length;
	Base(int l)
	{
		length=l;
		
	}
}
class Child extends Base
{
	int width;
	Child(int l,int w)
	{
		super(l);
		width=w;
		
		
	}
	

	int sum()
	{
		return(length*width);
	}

}
public class Constructor {

	public static void main(String[] args) {
		
		Child src=new Child(90,33);
		int s= src.sum();
		System.out.println(""+s);

	}

}


/**
OUTPUT
-----
2970

*/
