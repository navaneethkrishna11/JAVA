import java.util.*;
public class StringgTokenizer {
public static void main(String[]args)
{int sum=0;
	Scanner sr=new Scanner(System.in);
	
	System.out.println("enter the numbers with a space between each letter : ");
	String n=sr.nextLine();
	
	StringTokenizer st =new StringTokenizer(n);
	
	while(st.hasMoreTokens())
	{
		
	String a= st.nextToken();
	int r=Integer.parseInt(a);
	System.out.println(r);
	
	sum+=r;
	
}  System.out.println(" SUM : ");
	System.out.println(+sum);
}
}


/*
output
-------------
enter the numbers with a space between each letter : 
3 6 9 11 12 13
3
6
9
11
12
13
 SUM : 
54
*/
